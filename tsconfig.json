{
  "include": ["src/**/*"],
  "compilerOptions": {
    "strict": true,
    "composite": true,
    "baseUrl": ".",
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "lib": ["DOM", "DOM.Iterable", "ESNext"],
    "paths": {
      "@/*": ["./src/*"]
    },
    "noEmit": true
  }
}