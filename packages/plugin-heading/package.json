{
  "name": "@effitor/plugin-heading",
  "version": "0.2.1",
  "description": "A heading plugin for Effitor.",
  "keywords": [
    "editor",
    "wysiwyg",
    "markdown"
  ],
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./themes/*": "./dist/themes/*"
  },
  "author": "Henvn Leaf",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ausprain/effitor.git",
    "directory": "packages/plugin-heading"
  },
  "scripts": {
    "build": "tsup"
  },
  "dependencies": {
    "@effitor/core": "workspace:*"
  },
  "devDependencies": {
    "@effitor/assist-dropdown": "workspace:*"
  }
}
